package ie.gmit.sw.ds.RMI;

import java.rmi.Naming;
import java.util.List;

import ie.gmit.sw.ds.Models.Booking;

public class RmiConnector {
	
	private InterfaceRMI ifRmi;
	
	public RmiConnector() throws Exception{
		ifRmi = (InterfaceRMI) Naming.lookup("rmi://127.0.0.1:1099/myresource");
	}
	
	// ============== READ ====================
	public List<Booking> readData() throws Exception {
		return ifRmi.readCarHire();
	}
}
